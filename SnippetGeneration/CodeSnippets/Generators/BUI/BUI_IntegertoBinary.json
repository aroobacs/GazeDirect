        "public class E {",
        "    static void method(int num) {",
        "        int[] binaryNum = new int[32];",
        "        int i = 0;",
        "        while (num > 0) {",
        "            binaryNum[i] = num % 2;",
        "            num = num / 2;",
        "            i++;",
        "        }",
        "        for (int j = i - 1; j >= 0; j--)",
        "            System.out.print(binaryNum[j]);",
        "    }",
        "",
        "    public static void main(String[] args) {",
        "        int n = 17;",
        "        method(n);",
        "    }",
        "}",
        ""
    ]
}